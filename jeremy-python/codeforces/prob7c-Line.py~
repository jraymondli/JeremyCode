line = raw_input().split(' ')
A = int(line[0])
B = int(line[1])
C = int(line[2])
if A == 0:
    print 0, -C/B
elif B == 0:
    print -C/A, 0
elif C == 0:
    print 0, 0
else:
    x = 0
    y = float(-C-A*x)/float(B)
    while y != int:
        y = float(-C-A*x)/float(B)
        if y - int(y) == 0:
            break
        x = x + 1
    print x, int(y)
